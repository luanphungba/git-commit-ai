{
  "name": "@phungbaluan/commit-ai",
  "version": "1.0.5",
  "description": "AI-powered git commit message generator with security checks",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "commit-ai": "./dist/index.js",
    "cai": "./dist/index.js"
  },
  "scripts": {
    "start": "npx tsx src/index.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "watch": "tsc --watch"
  },
  "keywords": [
    "git",
    "commit",
    "ai",
    "openai",
    "cli",
    "security"
  ],
  "author": "Luan Phung",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luanphungba/git-commit-ai.git"
  },
  "bugs": {
    "url": "https://github.com/luanphungba/git-commit-ai/issues"
  },
  "homepage": "https://github.com/luanphungba/git-commit-ai#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^11.0.1",
    "dotenv": "^16.3.1",
    "openai": "^4.0.0",
    "simple-git": "^3.22.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/conf": "^3.0.3",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
